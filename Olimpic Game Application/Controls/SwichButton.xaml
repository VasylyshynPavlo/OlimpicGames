<UserControl x:Class="Olimpic_Game_Application.Controls.SwichButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Olimpic_Game_Application.Controls"
             mc:Ignorable="d" 
             Height="20" Width="30">
    <Grid>
        <Border Background="{DynamicResource FontColor}" CornerRadius="10"></Border>
        <Border Background="{StaticResource IndicatorColor}" CornerRadius="10" Width="20" HorizontalAlignment="Left"></Border>
    </Grid>
</UserControl>
